{
  "version": 2,
  "installCommand": "cd client && pnpm install --frozen-lockfile=false",
  "buildCommand": "cd client && pnpm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}


